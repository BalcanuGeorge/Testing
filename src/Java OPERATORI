
public class Java3 {

    public static void main(String[] args) {
        //OPERATORI MATEMATICI
        //+ - * / %
        System.out.println(100+100);
        System.out.println(100-50);
        System.out.println(100*3);
        System.out.println(100/4);
        System.out.println(10%3);
        System.out.println();

        //OPERATORI RATIONALI
        System.out.println(7==7);
        System.out.println(7==8);
        System.out.println(7!=7);
        System.out.println(7!=8);
        System.out.println(3+3 < 10);
        System.out.println(3+3 > 10);
        System.out.println(3>=4);
        System.out.println(3<=4);
        System.out.println();

        //OPERATORI LOGICI
        System.out.println(3>2 && 3 >1);
        System.out.println(3==3 || 3<2);
        System.out.println(!true);
        System.out.println(!false);
        System.out.println();

        //OPERATORI DE ATRIBUIRE
        int numar = 5;
        numar++;
        System.out.println(numar);

        int numar2 =5;
        numar--;
        System.out.println(numar2);


        int numar3 = 10;
        numar3 +=20;
        System.out.println(numar3);

        int numar4 = 10;
        numar4 -=5;
        System.out.println(numar4);

        int numar5 = 10;
        numar5 /=2;
        System.out.println(numar5);

        int numar6 = 10;
        numar6 *=2;
        System.out.println(numar6);

        numar = numar +15;
        System.out.println(numar);








    }
}



